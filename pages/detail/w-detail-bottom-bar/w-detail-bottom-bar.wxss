/* pages/detail/w-detail-bottom-bar/w-detail-bottom-bar.wxss */
.detail-bottom-bar{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 120rpx;
  width: 100%;
  background-color: #FFFFFF;
  
  border-top: 4rpx solid #eee;
  
  text-align: center;
  display: grid;
  grid-template-columns: repeat(3,15%) 30% 25%;
}

.addCart{
  line-height: 120rpx;
  background-color:#fff200;
  color: #000000;
}

.toBuy{
  line-height: 120rpx;
  
  background-color: #ff5777;
  color: #FFFFFF;
}














/*iconfont样式*/

@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
  url(data:application/font-woff;charset=utf-8;base64,) format('woff');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-kefu:before {
  content: "\e657";
}

.icon-dianpu:before {
  content: "\e655";
}

.icon-cart-full:before {
  content: "\e746";
  font-size: 45rpx;
}


